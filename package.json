{
  "name"            : "consolify",
  "version"         : "2.0.0",
  "keywords"        : ["browser", "console", "stdout", "ansi", "color", "browserify-plugin"],
  "description"     : "Turn your browser window into a console",
  "author"          : "Maximilian Antoni (http://maxantoni.de)",
  "homepage"        : "https://github.com/mantoni/consolify",
  "main"            : "./lib/consolify.js",
  "browser"         : "./lib/browser.js",
  "engines"         : {
    "node"          : ">=0.10"
  },
  "scripts"         : {
    "watch"         : "mocha --watch",
    "lint"          : "jslint --editon=latest --color \"**/*.js\"",
    "pretest"       : "npm run lint",
    "test"          : "mocha"
  },
  "dependencies"    : {
    "through2"      : "^1.1",
    "browser-reload": "^1.0",
    "brout"         : "^1.0",
    "source-mapper" : "^1.0",
    "ansi_up"       : "^1.1"
  },
  "devDependencies" : {
    "jslint"        : "^0.8",
    "mocha"         : "^2.2",
    "browserify"    : "^10.2"
  },
  "repository"      : {
    "type"          : "git",
    "url"           : "https://github.com/mantoni/consolify.git"
  },
  "files"           : ["bin", "lib", "README.md", "CHANGES.md", "LICENSE"],
  "license"         : "MIT"
}
